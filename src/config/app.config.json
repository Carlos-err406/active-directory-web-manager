{
	"$schema": "./config.schema.json",
	"system": {
		"logging": {
			"useLogging": true,
			"decodeSearchParams": true,
			"basePath": "./logs/",
			"logTemplate": "[{date}] {url}{urlSearchParams} {method} {status}",
			"logDateTemplate": "YYYY-MM-DD HH:mm:ss A"
		}
	},
	"app": {
		"captchaLength": 1,
		"views": {
			"logsPage": {
				"show": true
			}
		}
	},
	"directory": {
		"users": {
			"limit": null,
			"hide": []
		},
		"groups": {
			"limit": null,
			"hide": []
		}
	}
}
