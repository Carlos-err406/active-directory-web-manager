{
	"$schema": "./app.config.schema.json",
	"system": {
		"logging": {
			"use_logging": true,
			"paths": {
				"app_logs_path": "./logs/app/",
				"system_logs_path": "./logs/system/"
			}
		}
	},
	"app": {
		"captcha_length": 1
	},
	"directory": {
		"users": {
			"limit": 3,
			"hide": ["Guest", "CN=krbtgt,CN=Users,DC=local,DC=com"]
		},
		"groups": {
			"limit": null,
			"hide": []
		}
	}
}
